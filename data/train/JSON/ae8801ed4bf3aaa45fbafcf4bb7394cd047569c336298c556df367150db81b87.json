        {
            "bbox": [
                124.8699667004337,
                275.6579088967288,
                101.1095729751939,
                19.248592908295336
            ],
            "text": "erbracht hat,",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
