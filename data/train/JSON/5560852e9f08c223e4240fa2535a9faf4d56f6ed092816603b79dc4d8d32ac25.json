{
    "metadata": {
        "page_hash": "5560852e9f08c223e4240fa2535a9faf4d56f6ed092816603b79dc4d8d32ac25",
        "original_filename": "1709.08791.pdf",
        "page_no": 2,
        "num_pages": 30,
        "original_width": 612,
