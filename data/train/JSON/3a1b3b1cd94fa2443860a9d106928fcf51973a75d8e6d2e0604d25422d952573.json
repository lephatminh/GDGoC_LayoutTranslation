            "text": "subject to the following conditions: ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ArialMT",
                "size": 11.03999996185303
            },
            "text_vi": "tuân theo các điều kiện sau: "
