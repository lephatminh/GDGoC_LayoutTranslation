        {
            "bbox": [
                244.04271284279528,
                594.9445922996149,
                201.2036984108765,
                14.986388611421262
            ],
            "text": "accuracy of these data. ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ArialMT",
                "size": 11.03999996185303
            },
            "text_vi": "Độ chính xác của những dữ liệu này. "
        },
        {
            "bbox": [
                170.90908803821324,
                625.7705744196735,
                513.0081181044125,
                14.959519371425301
            ],
            "text": "(article introduced on 1.06.06; modified on 1.07.12; 1.01.15). ",
