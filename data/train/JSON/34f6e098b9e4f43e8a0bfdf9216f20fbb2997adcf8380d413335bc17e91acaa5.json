{
    "metadata": {
        "page_hash": "34f6e098b9e4f43e8a0bfdf9216f20fbb2997adcf8380d413335bc17e91acaa5",
        "original_filename": "aviation_instructors_handbook.pdf",
        "page_no": 79,
        "num_pages": 228,
