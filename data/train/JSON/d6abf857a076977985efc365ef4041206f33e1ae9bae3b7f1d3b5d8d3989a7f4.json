{
    "metadata": {
        "page_hash": "d6abf857a076977985efc365ef4041206f33e1ae9bae3b7f1d3b5d8d3989a7f4",
        "original_filename": "1002.2541.pdf",
        "page_no": 1,
        "num_pages": 13,
        "original_width": 612,
        "original_height": 792,
        "coco_width": 1025,
