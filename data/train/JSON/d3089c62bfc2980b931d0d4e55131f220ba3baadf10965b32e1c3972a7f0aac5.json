{
    "metadata": {
        "page_hash": "d3089c62bfc2980b931d0d4e55131f220ba3baadf10965b32e1c3972a7f0aac5",
        "original_filename": "1104.5538.pdf",
        "page_no": 1,
        "num_pages": 7,
        "original_width": 612,
        "original_height": 792,
        "coco_width": 1025,
        "coco_height": 1025,
