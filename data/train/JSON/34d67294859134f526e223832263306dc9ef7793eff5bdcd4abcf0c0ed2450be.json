            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/BCDHEE+Verdana-Italic",
                "size": 9.960000038146973
            },
            "text_vi": "Thực hiện hợp đồng "
        },
        {
            "bbox": [
                354.6315367427344,
                240.58156245442706,
                602.7271592970644,
                14.69654901170054
            ],
