            "bbox": [
                671.4762773692811,
                858.4032039141414,
                229.66299060457504,
                15.487788825757548
            ],
            "text": "The effect of band mixing",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/HXITUJ+CMR12",
                "size": 11.9552001953125
            },
            "text_vi": "Ảnh hưởng của việc trộn dải"
