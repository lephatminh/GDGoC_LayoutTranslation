            "bbox": [
                187.58169934640523,
                467.8956755050506,
                87.49348080065363,
                11.971275252525288
            ],
            "text": "information.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "thông tin."
