        },
        {
            "bbox": [
                93.18181818181817,
                434.2381348039215,
                48.96394198232322,
                22.35156535947715
            ],
            "text": "The ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/GSQFLF+AGaramondPro-Regular",
                "size": 1
            },
            "text_vi": "Các "
        },
        {
            "bbox": [
                142.5642966540404,
                434.2381348039215,
                52.72538655303032,
                22.07019281045757
            ],
            "text": "yield ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/GSQFLF+AGaramondPro-Italic",
                "size": 1
