{
    "metadata": {
        "page_hash": "ea21403f81b5d44f20a0c02136a857376084d8cb634a26ea13e4d52fa5c310a3",
        "original_filename": "0000004904-21-000010_20210225x10k.pdf",
        "page_no": 20,
        "num_pages": 476,
        "original_width": 595,
        "original_height": 842,
