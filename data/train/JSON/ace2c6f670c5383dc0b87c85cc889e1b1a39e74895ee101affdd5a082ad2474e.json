        {
            "bbox": [
                169.66819269548915,
                474.0306750403179,
                540.3712696715472,
                10.225528154206359
            ],
            "text": "$VERSION = sprintf \"%d.%03d\", q$Revision: 1.1 $ =~ /(\\d+)/g;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ Phiên bản = sprintf \"%d.%03d\", q $ sửa đổi: 1.1 $ = ~ /(\\ d+) /g;"
        },
        {
            "bbox": [
                169.66819269548915,
