            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Giả sử một số/mô -đun.pm"
        },
        {
            "bbox": [
                133.64279418286642,
                327.91503904411456,
                99.0712548464914,
                10.225528154206245
            ],
            "text": "use strict;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "sử dụng nghiêm ngặt;"
        },
        {
            "bbox": [
                133.64279418286642,
                342.4702482810077,
                117.08420808162938,
                10.225540329193223
            ],
            "text": "use warnings;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "sử dụng cảnh báo;"
        },
        {
            "bbox": [
                133.64279418286642,
                371.8618846065176,
                63.04534837621543,
                10.225528154206359
