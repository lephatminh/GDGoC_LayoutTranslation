            "bbox": [
                858.1834607843138,
                506.95280871212117,
                41.40169281045746,
                15.487801767676842
            ],
            "text": "anyi,",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/LFAYWW+CMR12",
                "size": 11.9552001953125
            },
            "text_vi": "Anyi,"
        },
        {
            "bbox": [
                158.3700401143791,
                513.3196893939394,
                10.541585702614356,
                5.399795770202047
            ],
            "text": "24",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
