        },
        {
            "bbox": [
                770.2205902777778,
                547.9519286616162,
                101.0254403594771,
                11.44941824494947
            ],
            "text": "that there is a",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/XFBJHD+CMR10",
                "size": 9.962599754333496
            },
            "text_vi": "rằng có một"
        },
        {
            "bbox": [
                153.75176862745099,
                573.7451606691919,
                337.5523032679738,
                11.449418244949527
            ],
            "text": "one on the last column of lookup tables. When",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/XFBJHD+CMR10",
                "size": 9.962599754333496
            },
            "text_vi": "Một trên cột cuối cùng của bảng tra cứu. Khi"
