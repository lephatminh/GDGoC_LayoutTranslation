        },
        {
            "bbox": [
                611.9824044519846,
                75.66305435275217,
                287.5777480988189,
                15.641400950056891
            ],
            "text": "insurance for the payment ofloss whether the person ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 1
            },
            "text_vi": "bảo hiểm cho việc thanh toán của những người có "
        },
        {
            "bbox": [
                612.262271788645,
                96.31172304662562,
                287.3659829091647,
                15.641400950057005
            ],
            "text": "insured has or has not any interest in the property ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 1
            },
            "text_vi": "được bảo hiểm có hoặc không có bất kỳ lợi ích nào đối với tài sản "
