            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Whitney-Book",
                "size": 1
            },
            "text_vi": "của nước Mỹ. Cũng theo ý kiến ​​của chúng tôi, công ty duy trì, trong tất cả"
        },
        {
            "bbox": [
                70.34313725490196,
                255.27587468434342,
                411.20233169934636,
                13.266735479798058
            ],
