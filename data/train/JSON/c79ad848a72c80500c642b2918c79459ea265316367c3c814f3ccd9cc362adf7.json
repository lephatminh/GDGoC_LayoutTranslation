            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/MENEHF+NissanAG-Medium",
                "size": 1
            },
            "text_vi": "Bộ phận bộ phận. Mục tiêu chính của chúng tôi là mở rộng"
