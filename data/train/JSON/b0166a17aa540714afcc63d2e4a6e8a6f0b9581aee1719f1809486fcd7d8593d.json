        {
            "bbox": [
                491.7929292929293,
                655.8940568475452,
                211.03785395622896,
                15.917945413436712
            ],
            "text": "sideward or rearward motion. ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WUSXGL+MyriadPro-Regular",
                "size": 1
            },
            "text_vi": "chuyển động sang một bên hoặc phía sau. "
        },
        {
            "bbox": [
                491.7929292929293,
                689.001265503876,
                413.98595580808075,
                15.917971899224824
            ],
            "text": "All helicopter pilots should receive training on avoidance ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WUSXGL+MyriadPro-Regular",
                "size": 1
            },
            "text_vi": "Tất cả các phi công trực thăng nên được đào tạo về tránh "
        },
        {
            "bbox": [
                491.7929292929293,
                705.5548830749353,
                415.2647727272726,
                15.917971899224824
            ],
            "text": "and recovery from inadvertent IMC with emphasis on ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WUSXGL+MyriadPro-Regular",
                "size": 1
