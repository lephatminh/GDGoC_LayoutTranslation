{
    "metadata": {
        "page_hash": "e47be5e969201a4bd2b8925528c882ffe4f5d807c9556365c37fc10e4b3b2a66",
        "original_filename": "NASDAQ_ERIE_2012.pdf",
        "page_no": 6,
        "num_pages": 14,
