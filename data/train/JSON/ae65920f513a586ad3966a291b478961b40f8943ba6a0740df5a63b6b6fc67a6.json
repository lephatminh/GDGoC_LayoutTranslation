            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 1
            },
            "text_vi": "Hàng đợi bản sao hoãn lại"
        },
        {
            "bbox": [
                229.11763031045754,
                191.27941729797988,
                8.340669526143756,
                11.356249368686917
            ],
