            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/QTJBAA+DejaVuSerif",
                "size": 15
            },
            "text_vi": "Số dư phải thu được tạo ra từ bán hàng cho các nhà phân phối độc lập mà chúng tôi có ngày thu thập được xác định trước"
        },
        {
            "bbox": [
                24.738352605042017,
                272.9028292755345,
                940.9472747058824,
                12.23997298099755
            ],
