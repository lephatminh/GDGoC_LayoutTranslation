            "bbox": [
                644.3096572712419,
                374.49383175505056,
                271.32352941176464,
                10.17237436868686
            ],
            "text": "innodb_autoextend_increment",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Innodb_AutoExtend_increment"
        },
        {
            "bbox": [
                157.4346405228758,
                388.8723298611111,
                767.9461119281046,
                11.971275252525288
