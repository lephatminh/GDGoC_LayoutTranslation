    "cells": [
        {
            "bbox": [
                382.84254125816994,
                48.370422979798036,
                289.46200980392155,
                11.971275252525288
            ],
            "text": "Managing MySQL Server with systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Quản lý máy chủ MySQL với SystemD"
        },
        {
            "bbox": [
                170.83333333333334,
                94.96133207070716,
                787.5081866830066,
                11.971275252525174
            ],
            "text": "If you install MySQL using an RPM or Debian package on the following Linux platforms, server startup and",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Nếu bạn cài đặt MYSQL bằng gói RPM hoặc Debian trên các nền tảng Linux, máy chủ khởi động sau đây và"
        },
        {
            "bbox": [
                170.83333333333334,
                110.49163510101016,
                256.91991789215683,
                11.971275252525288
            ],
            "text": "shutdown is managed by systemd:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Tắt máy được quản lý bởi Systemd:"
        },
        {
            "bbox": [
                170.83333333333334,
                138.82277746212128,
                200.109493872549,
                11.971275252525174
            ],
            "text": "•RPM package platforms:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Nền tảng gói RPM:"
        },
        {
            "bbox": [
                187.58169934640523,
                167.1539198232323,
                357.4435939542484,
                11.971275252525288
            ],
            "text": "•Enterprise Linux variants version 7 and higher",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Biến thể Linux doanh nghiệp phiên bản 7 trở lên"
        },
        {
            "bbox": [
                187.58169934640523,
                195.4850621843434,
                349.0694109477124,
                11.971275252525288
            ],
            "text": "•SUSE Linux Enterprise Server 12 and higher",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• SUSE Linux Enterprise Server 12 trở lên"
        },
        {
            "bbox": [
                187.58169934640523,
                223.8162951388889,
                176.8794934640523,
                11.971275252525288
            ],
            "text": "•Fedora 29 and higher",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Fedora 29 trở lên"
        },
        {
            "bbox": [
                170.83333333333334,
                252.14743750000002,
                196.37459150326796,
                11.971275252525288
            ],
            "text": "•Debian family platforms:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Nền tảng gia đình Debian:"
        },
        {
            "bbox": [
                187.58169934640523,
                280.47857986111114,
                143.3325163398693,
                11.971275252525288
            ],
            "text": "•Debian platforms",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Nền tảng Debian"
        },
        {
            "bbox": [
                187.58169934640523,
                308.80972222222226,
                144.2704248366013,
                11.971275252525174
            ],
            "text": "•Ubuntu platforms",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Nền tảng Ubuntu"
        },
        {
            "bbox": [
                170.83333333333334,
                337.14086458333327,
                778.1793300653594,
                11.971275252525288
            ],
            "text": "If you install MySQL from a generic binary distribution on a platform that uses systemd, you can manually",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Nếu bạn cài đặt MySQL từ phân phối nhị phân chung trên nền tảng sử dụng systemd, bạn có thể thủ công"
        },
        {
            "bbox": [
                170.83333333333334,
                352.6711676136364,
                749.3386940359477,
                11.971275252525288
            ],
            "text": "configure systemd support for MySQL following the instructions provided in the post-installation setup",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Định cấu hình Hỗ trợ Systemd cho MySQL theo các hướng dẫn được cung cấp trong thiết lập sau khi cài đặt"
        },
        {
            "bbox": [
                170.83333333333334,
                368.2014706439395,
                393.7708165849673,
                11.971275252525174
            ],
            "text": "section of the MySQL 8.0 Secure Deployment Guide.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Phần của Hướng dẫn triển khai an toàn MySQL 8.0."
        },
        {
            "bbox": [
                170.83333333333334,
                396.53265183080805,
                790.2883819444445,
                11.971275252525288
            ],
            "text": "If you install MySQL from a source distribution on a platform that uses systemd, obtain systemd support for",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Nếu bạn cài đặt MySQL từ phân phối nguồn trên nền tảng sử dụng SystemD, hãy lấy hỗ trợ Systemd cho"
        },
        {
            "bbox": [
                170.83333333333334,
                412.06295486111117,
                355.60132393790855,
                11.971275252525174
            ],
            "text": "MySQL by configuring the distribution using the ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "MySQL bằng cách định cấu hình phân phối bằng cách sử dụng "
        },
        {
            "bbox": [
                526.4346572712419,
                413.2147727272727,
                215.68545751633985,
                10.17236142676768
            ],
            "text": "-DWITH_SYSTEMD=1 CMake",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "-Dwith_systemd = 1 cmake"
        },
        {
            "bbox": [
                742.1201147875818,
                412.06295486111117,
                196.45835008169934,
                11.971275252525174
            ],
            "text": " option. See Section 2.9.7,",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " lựa chọn. Xem Phần 2.9.7,"
        },
        {
            "bbox": [
                170.83333333333334,
                427.59325789141417,
                319.82683088235297,
                11.971275252525288
            ],
            "text": "“MySQL Source-Configuration Options”",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "“Các tùy chọn cấu hình nguồn MySQL nguồn”"
        },
        {
            "bbox": [
                463.0923202614379,
                427.59325789141417,
                4.65604575163394,
                11.971275252525288
            ],
            "text": ".",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "."
        },
        {
            "bbox": [
                170.83333333333334,
                455.9244002525253,
                334.1634321895425,
                11.971275252525288
            ],
            "text": "The following discussion covers these topics:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Các cuộc thảo luận sau đây bao gồm các chủ đề này:"
        },
        {
            "bbox": [
                170.83333333333334,
                484.2555426136364,
                172.1732026143791,
                11.971275252525288
            ],
            "text": "•Overview of systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Tổng quan về Systemd"
        },
        {
            "bbox": [
                170.83333333333334,
                512.5867367424243,
                254.08947753267975,
                11.971275252525288
            ],
            "text": "•Configuring systemd for MySQL",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Định cấu hình SystemD cho MySQL"
        },
        {
            "bbox": [
                170.83333333333334,
                540.9178791035354,
                416.04616013071893,
                11.971275252525288
            ],
            "text": "•Configuring Multiple MySQL Instances Using systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Định cấu hình nhiều phiên bản MySQL bằng SystemD"
        },
        {
            "bbox": [
                170.83333333333334,
                569.2490214646465,
                308.06944444444446,
                11.971275252525231
            ],
            "text": "•Migrating from mysqld_safe to systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "• Di chuyển từ mysqld_safe sang systemd"
        },
        {
            "bbox": [
                311.8545751633987,
                597.5802026515153,
                37.214869281045765,
                11.971275252525231
            ],
            "text": "Note",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica-Bold",
                "size": 10
            },
            "text_vi": "Ghi chú"
        },
        {
            "bbox": [
                311.8545751633987,
                625.9113450126263,
                582.6589554738562,
                11.971275252525288
            ],
            "text": "On platforms for which systemd support for MySQL is installed, scripts such as",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Trên các nền tảng hỗ trợ SystemD cho MySQL được cài đặt, các tập lệnh như"
        },
        {
            "bbox": [
                311.8545751633987,
                642.5934659090909,
                110.53921568627447,
                10.17236142676768
            ],
            "text": "mysqld_safe",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "mysqld_safe"
        },
        {
            "bbox": [
                422.39379084967317,
                641.4416480429293,
                490.576372140523,
                11.971275252525231
            ],
            "text": " and the System V initialization script are unnecessary and are not",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " và tập lệnh khởi tạo hệ thống v không cần thiết và không"
        },
        {
            "bbox": [
                311.8545751633987,
                656.9719510732323,
                173.12787622549024,
                11.971275252525288
            ],
            "text": "installed. For example, ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "đã cài đặt. Ví dụ, "
        },
        {
            "bbox": [
                484.98245138888893,
                658.123768939394,
                110.53921568627453,
                10.17236142676768
            ],
            "text": "mysqld_safe",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "mysqld_safe"
        },
        {
            "bbox": [
                595.5216670751635,
                656.9719510732323,
                297.8697397875816,
                11.971275252525288
            ],
            "text": " can handle server restarts, but systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " có thể xử lý khởi động lại máy chủ, nhưng systemd"
        },
        {
            "bbox": [
                311.8545751633987,
                672.5022541035354,
                621.8332998366013,
                11.971275252525231
            ],
            "text": "provides the same capability, and does so in a manner consistent with management",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "cung cấp khả năng tương tự và làm như vậy theo cách phù hợp với quản lý"
        },
        {
            "bbox": [
                311.8545751633987,
                688.0325571338384,
                516.6200477941177,
                11.971275252525288
            ],
            "text": "of other services rather than by using an application-specific program.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "của các dịch vụ khác thay vì sử dụng chương trình dành riêng cho ứng dụng."
        },
        {
            "bbox": [
                311.8545751633987,
                716.3636994949495,
                584.5849505718954,
                11.971275252525231
            ],
            "text": "Because systemd has the capability of managing multiple MySQL instances on",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Bởi vì SystemD có khả năng quản lý nhiều phiên bản MySQL"
        },
        {
            "bbox": [
                311.8545751633987,
                731.8940025252525,
                444.9036450163399,
                11.971275252525288
            ],
            "text": "platforms for which systemd support for MySQL is installed, ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "các nền tảng hỗ trợ Systemd cho MySQL được cài đặt, "
        },
        {
            "bbox": [
                756.7581699346405,
                733.0458203914142,
                120.58818504901967,
                10.17236142676768
            ],
            "text": "mysqld_multi",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "mysqld_multi"
        },
        {
            "bbox": [
                877.3463549836601,
                731.8940025252525,
                32.59235375816991,
                11.971275252525288
            ],
            "text": " and",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Và"
        },
        {
            "bbox": [
                311.8545751633987,
                748.5761622474747,
                190.9313892973857,
                10.17236142676768
            ],
            "text": "mysqld_multi.server",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "mysqld_multi.server"
        },
        {
            "bbox": [
                502.7859644607844,
                747.4243443813132,
                288.59109477124184,
                11.971275252525231
            ],
            "text": " are unnecessary and are not installed.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " không cần thiết và không được cài đặt."
        },
        {
            "bbox": [
                90.44117647058823,
                776.1113506944445,
                201.0607843137255,
                14.365569128787854
            ],
            "text": "Overview of systemd",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica-Bold",
                "size": 12
            },
            "text_vi": "Tổng quan về Systemd"
        },
        {
            "bbox": [
                170.83333333333334,
                807.1926508838384,
                712.0903447712418,
                11.971275252525231
            ],
            "text": "systemd provides automatic MySQL server startup and shutdown. It also enables manual server",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Systemd cung cấp tự động khởi động và tắt máy chủ MySQL. Nó cũng cho phép máy chủ thủ công"
        },
        {
            "bbox": [
                170.83333333333334,
                822.7229539141415,
                175.02042483660128,
                11.971275252525231
            ],
            "text": "management using the ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Quản lý sử dụng "
        },
        {
            "bbox": [
                345.8537581699346,
                823.8747976641414,
                90.44117647058829,
                10.17233554292929
            ],
            "text": "systemctl",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Systemctl"
        },
        {
            "bbox": [
                436.2949346405229,
                822.7229539141415,
                185.22017973856202,
                11.971275252525231
            ],
            "text": " command. For example:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " yêu cầu. Ví dụ:"
        },
        {
            "bbox": [
                170.83333333333334,
                850.1520962752526,
                56.27452655228757,
                8.137827020201996
            ],
            "text": "shell> ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "Vỏ> "
        },
        {
            "bbox": [
                227.1078598856209,
                850.1520962752526,
                80.39215686274511,
                8.137827020201996
            ],
            "text": "systemctl ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier-Bold",
                "size": 8
