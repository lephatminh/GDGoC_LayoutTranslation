            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "trên máy lưu trữ nút quản lý, sử dụng "
        },
        {
            "bbox": [
                568.0208165849673,
                382.0195577651515,
                90.44117647058829,
                10.172374368686974
            ],
            "text": "--install",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "--cài đặt"
        },
        {
            "bbox": [
                658.4619930555556,
                380.86775284090913,
                173.16133946078435,
                11.971275252525288
            ],
            "text": " option, as shown here:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Tùy chọn, như được hiển thị ở đây:"
        },
        {
            "bbox": [
                140.68627450980392,
                408.51558775252533,
                40.19607843137254,
                8.137827020202053
            ],
            "text": "C:\\> ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "C: \\> "
        },
        {
            "bbox": [
                180.88235294117646,
                408.51558775252533,
                281.37254901960785,
                8.137827020202053
            ],
            "text": "C:\\mysql\\bin\ndb_mgmd.exe --install",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier-Bold",
                "size": 8
            },
            "text_vi": "C: \\ mysql \\ bin"
        },
        {
            "bbox": [
                140.68627450980392,
                420.9397913510102,
                401.96078431372547,
                8.137917613636318
            ],
            "text": "Installing service ’NDB Cluster Management Server’",
            "font": {
                "color": [
                    0,
                    0,
                    0,
