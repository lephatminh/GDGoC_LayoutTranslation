        },
        {
            "bbox": [
                331.277105882353,
                331.14543527315914,
                138.9886735294117,
                11.460936460807602
            ],
            "text": "may be found under",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/UVLVGJ+NimbusRomNo9L-Regu",
                "size": 9.962639808654785
            },
            "text_vi": "có thể được tìm thấy dưới"
        },
        {
            "bbox": [
                475.01673193277315,
                332.67112707838476,
                185.42582478991596,
                9.321522862232769
            ],
