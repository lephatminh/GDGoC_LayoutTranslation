            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/KPBZFY+CMMI12",
                "size": 11.9552001953125
            },
            "text_vi": "U"
        },
        {
            "bbox": [
                761.9796941176471,
                395.6029985154395,
                35.75341260504206,
                14.568135095011826
            ],
            "text": "(1)$_{L}$",
