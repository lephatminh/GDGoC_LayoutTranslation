            "bbox": [
                666.9621566298658,
                471.0052759742332,
                72.05176128384153,
                10.22554032919345
            ],
            "text": "print $b",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "In $ b"
        },
        {
            "bbox": [
                743.3004606266674,
                470.9203797892791,
                97.0126626136447,
                10.91692132087428
            ],
            "text": "would cause a",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "sẽ gây ra a"
