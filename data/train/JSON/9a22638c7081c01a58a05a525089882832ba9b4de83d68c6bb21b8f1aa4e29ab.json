{
    "metadata": {
        "page_hash": "9a22638c7081c01a58a05a525089882832ba9b4de83d68c6bb21b8f1aa4e29ab",
        "original_filename": "1110.6560.pdf",
        "page_no": 4,
        "num_pages": 40,
