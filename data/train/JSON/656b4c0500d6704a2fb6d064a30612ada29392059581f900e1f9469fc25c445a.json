            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 1
            },
            "text_vi": "Cho phép 1 TB Công suất hàng đợi Active Premigration (FC 5274)."
