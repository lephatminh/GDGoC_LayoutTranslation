            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 1
            },
            "text_vi": "Bật công suất bộ đệm của đĩa tăng dần (chỉ TS7740)."
