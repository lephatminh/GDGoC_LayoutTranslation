{
    "metadata": {
        "page_hash": "a26ef8b7855e1d8dc6130684f9905fca1840dcd8935dc0d970feb82abbc4f4c6",
        "original_filename": "sailingprimer.pdf",
        "page_no": 11,
        "num_pages": 16,
        "original_width": 595,
        "original_height": 842,
        "coco_width": 1025,
