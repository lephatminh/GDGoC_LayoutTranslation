            "text_vi": "mong đợi:"
        },
        {
            "bbox": [
                120.58823529411765,
                236.7037806186869,
                441.50737785947706,
                8.958474116161597
            ],
            "text": "objdump -T <path to mongod>/mongod | grep \" SSL_\"",
            "font": {
                "color": [
                    0,
