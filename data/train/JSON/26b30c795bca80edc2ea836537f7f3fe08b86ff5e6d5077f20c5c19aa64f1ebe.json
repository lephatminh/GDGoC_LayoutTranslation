            "bbox": [
                399.8701497680008,
                265.8427423316259,
                19.14427777797283,
                10.916470846352013
            ],
            "text": "(",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ZKWHER+FreeMono",
                "size": 9.962599754333496
            },
            "text_vi": "."
        },
