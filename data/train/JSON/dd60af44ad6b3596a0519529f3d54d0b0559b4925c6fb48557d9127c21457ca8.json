            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Trên UNIX, SymLink Một cơ sở dữ liệu bằng quy trình này:"
        },
        {
            "bbox": [
                170.83333333333334,
                386.7796085858587,
                13.968137254901961,
                11.971275252525174
            ],
            "text": "1.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "1."
        },
        {
            "bbox": [
                197.63071895424838,
                386.7796085858587,
                201.09764746732026,
                11.971275252525174
            ],
            "text": "Create the database using ",
