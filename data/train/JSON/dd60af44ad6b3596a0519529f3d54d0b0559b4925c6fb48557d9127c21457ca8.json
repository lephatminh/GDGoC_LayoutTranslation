            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier-Bold",
                "size": 8
            },
            "text_vi": "Tạo cơ sở dữ liệu MyDB1;"
        },
        {
            "bbox": [
                197.63071895424838,
                440.4407010732324,
                47.464869281045736,
                11.971275252525174
            ],
            "text": "Using ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Sử dụng "
        },
        {
            "bbox": [
                245.09558823529412,
                441.59251893939404,
                150.7352941176471,
                10.172361426767566
            ],
            "text": "CREATE DATABASE",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Tạo cơ sở dữ liệu"
        },
        {
            "bbox": [
                395.8308823529412,
                440.4407010732324,
                563.1972871732027,
                11.971275252525174
            ],
            "text": " creates the database in the MySQL data directory and permits the server to",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Tạo cơ sở dữ liệu trong thư mục dữ liệu MySQL và cho phép máy chủ"
