            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "MySQL có được hỗ trợ trên các môi trường ảo hóa như Oracle VM, VMware, Docker, Microsoft"
        },
