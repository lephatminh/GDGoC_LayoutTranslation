            "bbox": [
                221.07843137254903,
                445.94619349747484,
                146.12949346405227,
                11.971275252525174
            ],
            "text": "Hyper-V, or others?",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Hyper-V, hay những người khác?"
