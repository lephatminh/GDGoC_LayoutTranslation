{
    "metadata": {
        "page_hash": "69949f2b2a3fb0e2e41c76461d46592162bf0f5ec20bf215313bb8c9c2d2962e",
        "original_filename": "1705.03918.pdf",
        "page_no": 2,
        "num_pages": 28,
        "original_width": 612,
