        },
        {
            "bbox": [
                136.21831339580297,
                491.8369954288921,
                121.97344513301391,
                10.91692132087428
            ],
            "text": "and 5.8.0, and the",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "và 5.8.0 và"
        },
        {
            "bbox": [
                262.4746163964279,
                491.8369954288921,
                87.68020632446127,
                10.91692132087428
            ],
