                813.4622922812275,
                477.2818227169604,
                112.88119082576839,
                10.916909145887075
            ],
            "text": "erences between",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "sự căng thẳng giữa"
        },
        {
            "bbox": [
                97.61911756059375,
                491.8369954288921,
                34.310392280219574,
                10.91692132087428
            ],
            "text": "5.6.0",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
