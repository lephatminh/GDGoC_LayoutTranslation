        },
        {
            "bbox": [
                179.95992615190266,
                369.10154718429317,
                408.8838017995015,
                13.099713880676518
            ],
            "text": "How do I strip blank space from the beginning",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Bold",
                "size": 11.95499992370605
            },
            "text_vi": "Làm cách nào để loại bỏ khoảng trống ngay từ đầu"
        },
        {
            "bbox": [
                588.8503400103482,
                369.10154718429317,
                5.7638901954723,
                13.099713880676518
            ],
            "text": "/",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/LAOJMD+rtxb",
                "size": 11.95499992370605
            },
