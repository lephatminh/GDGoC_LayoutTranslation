            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAA+DejaVuSansCondensed-Bold",
                "size": 10
            },
            "text_vi": "§ 20 Hoa hồng tài trợ liên tục"
