{
    "metadata": {
        "page_hash": "be6babfc5b45c540d01b9c94e3a2d6fb6171ba022c10d41d5ead4ac8311dbae9",
        "original_filename": "NYSE_ATI_2003.pdf",
        "page_no": 37,
        "num_pages": 74,
        "original_width": 612,
