{
    "metadata": {
        "page_hash": "a410a857537359c857591de7cf3460f20ea7be2808ac9c13b164b186928949c6",
        "original_filename": "NYSE_AXP_2002.pdf",
        "page_no": 23,
        "num_pages": 92,
        "original_width": 576,
        "original_height": 783,
        "coco_width": 1025,
        "coco_height": 1025,
