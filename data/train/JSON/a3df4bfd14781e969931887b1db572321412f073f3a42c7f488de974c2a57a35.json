        },
        {
            "bbox": [
                228.26349714052287,
                256.9302660984848,
                5.562383578431394,
                14.605952335858774
            ],
            "text": "|",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WXXMPB+CMSY10",
                "size": 11.9552001953125
            },
