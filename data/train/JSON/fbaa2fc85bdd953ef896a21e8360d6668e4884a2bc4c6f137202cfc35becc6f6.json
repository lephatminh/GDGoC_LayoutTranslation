            "bbox": [
                104.9921568627451,
                866.5262642045454,
                475.0104407679738,
                19.154027462121206
            ],
            "text": "Point-in-Time (Incremental) Recovery",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica-Bold",
                "size": 16
            },
            "text_vi": "Phục hồi điểm trong thời gian (gia tăng)"
