        },
        {
            "bbox": [
                604.1467573529412,
                433.902055239899,
                9.446095179738563,
                13.003095643939446
            ],
            "text": "ρ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/BLVAWQ+CMMI10",
                "size": 10.90909957885742
            },
            "text_vi": "ρ"
        },
        {
            "bbox": [
                620.6268815359477,
                433.2667234848485,
                280.4590261437909,
                14.11845612373736
            ],
            "text": "is one of Bell-diagonal, generalized",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/RPCANV+CMR10",
                "size": 10.90909957885742
            },
            "text_vi": "là một trong những đường chéo chuông, tổng quát"
