        },
        {
            "bbox": [
                120.58884828431373,
                433.2667234848485,
                92.94439395424835,
                14.11845612373736
            ],
            "text": "how to find",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/RPCANV+CMR10",
                "size": 10.90909957885742
            },
