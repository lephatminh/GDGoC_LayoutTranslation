            "bbox": [
                140.68627450980392,
                838.000954229798,
                56.274509803921575,
                8.137839962121205
            ],
            "text": "shell> ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
