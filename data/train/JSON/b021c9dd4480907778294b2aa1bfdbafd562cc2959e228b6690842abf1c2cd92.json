            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/UJRGAU+Times-Roman",
                "size": 1
            },
            "text_vi": "khoảng cách, nhưng thật bất thường khi thấy một đội bóng khác "
