            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Để biết thêm"
        },
        {
            "bbox": [
                187.58169934640523,
                200.04972885101017,
                87.49348080065363,
                11.971275252525288
