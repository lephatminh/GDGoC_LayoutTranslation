{
    "metadata": {
        "page_hash": "baf7fbaca5d4e3439c8adbc90d476f70875b6b87b560691f99a42c3551392c72",
        "original_filename": "NYSE_LMT_2011.pdf",
        "page_no": 85,
        "num_pages": 110,
        "original_width": 594,
        "original_height": 774,
