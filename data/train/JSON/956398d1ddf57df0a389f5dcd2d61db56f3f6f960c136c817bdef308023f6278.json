            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "TẮT"
        },
        {
            "bbox": [
                865.4047532679739,
                634.1889965277778,
                4.65609599673212,
                11.971275252525231
            ],
            "text": ",",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Thì"
        },
        {
            "bbox": [
                140.68627450980392,
                650.8711044823233,
                140.68627450980392,
                10.172374368686917
            ],
            "text": "OFF_PERMISSIVE",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
