            "bbox": [
                93.18181818181817,
                737.2682043946932,
                35.49243686868688,
                20.00791500829189
            ],
            "text": "15.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/RCIJBQ+MyriadPro-Bold",
                "size": 1
            },
