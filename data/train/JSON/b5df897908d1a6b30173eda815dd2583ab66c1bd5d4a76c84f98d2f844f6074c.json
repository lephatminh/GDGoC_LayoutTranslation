        {
            "bbox": [
                150.7352421977124,
                700.4614457070707,
                118.25185665849676,
                15.487788825757605
            ],
            "text": "the retraction",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/FEKMZD+CMR12",
                "size": 11.9552001953125
            },
            "text_vi": "sự rút lại"
        },
        {
            "bbox": [
                276.12186928104575,
