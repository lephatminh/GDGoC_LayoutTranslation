        },
        {
            "bbox": [
                146.70104581740236,
                71.7520201956072,
                106.3441374253288,
                10.832049485894345
            ],
            "text": "DESCRIPTION",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/PUMMVX+NimbusRomNo9L-Regu-Slant_167",
                "size": 9.963000297546387
            },
            "text_vi": "SỰ MIÊU TẢ"
        },
        {
            "bbox": [
                731.2634139457999,
                71.7520201956072,
                196.11831738554883,
                10.832049485894345
            ],
            "text": "CHAPTER 24. PERLFAQ8",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/PUMMVX+NimbusRomNo9L-Regu-Slant_167",
                "size": 9.963000297546387
            },
            "text_vi": "Chương 24. Perlfaq8"
        },
        {
            "bbox": [
                133.64279418286642,
                110.15205121628662,
                135.0971613167674,
                10.225528154206131
            ],
            "text": "use IPC::Open3;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Sử dụng IPC :: Open3;"
        },
        {
            "bbox": [
                133.64279418286642,
                124.70726045317952,
                198.14250624920206,
                10.22554032919345
            ],
            "text": "use Symbol qw(gensym);",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "sử dụng ký hiệu QW (GENSYM);"
        },
        {
            "bbox": [
                133.64279418286642,
                139.26248186505984,
                117.08420808162938,
                10.225528154206131
            ],
            "text": "use IO::File;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Sử dụng tệp IO ::;"
        },
        {
            "bbox": [
                133.64279418286642,
                153.81770327693994,
                369.248403345675,
                10.225528154206245
            ],
            "text": "local *CATCHOUT = IO::File->new_tempfile;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "cục bộ *bắt gặp = io :: file-> new_tempfile;"
        },
        {
            "bbox": [
                133.64279418286642,
                168.37291251383294,
                369.248403345675,
                10.225540329193336
            ],
            "text": "local *CATCHERR = IO::File->new_tempfile;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "cục bộ *Catcherr = io :: file-> new_tempfile;"
        },
        {
            "bbox": [
                133.64279418286642,
                182.92932707444754,
                531.3648963673994,
                10.225528154206359
            ],
            "text": "my $pid = open3(gensym, \">&CATCHOUT\", \">&CATCHERR\", \"cmd\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ pid = open3 của tôi (gensym, \"> & cougout\", \"> & catcherr\", \"cmd\");"
        },
        {
            "bbox": [
                133.64279418286642,
                197.48453631134066,
                153.11011455190535,
                10.22554032919345
            ],
            "text": "waitpid($pid, 0);",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Waitpid ($ pid, 0);"
        },
        {
            "bbox": [
                133.64279418286642,
                212.03975772322087,
                369.248403345675,
                10.225540329193336
            ],
            "text": "seek $_, 0, 0 for \\*CATCHOUT, \\*CATCHERR;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "tìm kiếm $ _, 0, 0 cho \\*bắt, \\*Catcherr;"
        },
        {
            "bbox": [
                133.64279418286642,
                226.59497913510097,
                198.14250624920206,
                10.225528154206359
            ],
            "text": "while( <CATCHOUT> ) {}",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "WHER"
        },
        {
            "bbox": [
                133.64279418286642,
                241.15020054698107,
                198.14250624920206,
                10.225528154206359
            ],
            "text": "while( <CATCHERR> ) {}",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "WHER"
        },
        {
            "bbox": [
                97.61913822327794,
                270.1172098478754,
                788.6071580661744,
                10.916994370796829
            ],
            "text": "But there’s no real need for *both* to be tempfiles... the following should work just as well, without deadlocking:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Nhưng ở đó, không có nhu cầu thực sự cho * cả hai * để trở thành tempfiles ... những điều sau đây cũng sẽ hoạt động tốt, mà không bị dừng lại:"
        },
        {
            "bbox": [
                133.64279418286642,
                299.25276966999525,
                135.0971613167674,
                10.225528154206245
            ],
            "text": "use IPC::Open3;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Sử dụng IPC :: Open3;"
        },
        {
            "bbox": [
                133.64279418286642,
                313.80797890688825,
                198.14250624920206,
                10.225540329193336
            ],
            "text": "use Symbol qw(gensym);",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "sử dụng ký hiệu QW (GENSYM);"
        },
        {
            "bbox": [
                133.64279418286642,
                328.3632003187686,
                117.08420808162938,
                10.225528154206131
            ],
            "text": "use IO::File;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Sử dụng tệp IO ::;"
        },
        {
            "bbox": [
                133.64279418286642,
                342.9184217306487,
                369.248403345675,
                10.225528154206245
            ],
            "text": "local *CATCHERR = IO::File->new_tempfile;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "cục bộ *Catcherr = io :: file-> new_tempfile;"
        },
        {
            "bbox": [
                133.64279418286642,
                357.4748241162762,
                513.3519431322615,
                10.225528154206245
            ],
            "text": "my $pid = open3(gensym, \\*CATCHOUT, \">&CATCHERR\", \"cmd\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ PID của tôi = Open3 (gensym, \\*bắt, \"> & Catcherr\", \"cmd\");"
        },
        {
            "bbox": [
                133.64279418286642,
                372.0300455281563,
                198.14250624920206,
                10.225528154206359
            ],
            "text": "while( <CATCHOUT> ) {}",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "WHER"
        },
        {
            "bbox": [
                133.64279418286642,
                386.5852669400364,
                153.11011455190535,
                10.225528154206359
            ],
            "text": "waitpid($pid, 0);",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Waitpid ($ pid, 0);"
        },
        {
            "bbox": [
                133.64279418286642,
                401.14040312700706,
                180.1295530140641,
                10.22557685415461
            ],
            "text": "seek CATCHERR, 0, 0;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Tìm kiếm Catcherr, 0, 0;"
        },
        {
            "bbox": [
                133.64279418286642,
                415.69562453888716,
                198.14250624920206,
                10.225528154206245
            ],
            "text": "while( <CATCHERR> ) {}",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "WHER"
        },
        {
            "bbox": [
                97.61913822327794,
                444.6626825397299,
                788.795911686344,
                10.916909145887075
            ],
            "text": "And it’ll be faster, too, since we can begin processing the program’s stdout immediately, rather than waiting for the",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Và nó cũng sẽ nhanh hơn, vì chúng ta có thể bắt đầu xử lý chương trình STDout ngay lập tức, thay vì chờ đợi"
        },
        {
            "bbox": [
                97.61913822327794,
                459.2178552516617,
                132.07784897257744,
                10.916921320874167
            ],
            "text": "program to finish.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "chương trình để hoàn thành."
        },
        {
            "bbox": [
                97.61913822327794,
                477.69338598043225,
                458.3868148976273,
                10.916909145887189
            ],
            "text": "With any of these, you can change file descriptors before the call:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Với bất kỳ trong số này, bạn có thể thay đổi mô tả tệp trước cuộc gọi:"
        },
        {
            "bbox": [
                133.64279418286642,
                506.82897015252627,
                225.1619447113608,
                10.22554032919328
            ],
            "text": "open(STDOUT, \">logfile\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Mở (stdout, \"> logfile\");"
        },
        {
            "bbox": [
                133.64279418286642,
                521.3841550394452,
                117.08420808162938,
                10.225528154206245
            ],
            "text": "system(\"ls\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "hệ thống (\"ls\");"
        },
        {
            "bbox": [
                97.61913822327794,
                550.349983366592,
                384.55088248980303,
                10.916921320874167
            ],
            "text": "or you can use Bourne shell file-descriptor redirection:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Hoặc bạn có thể sử dụng chuyển hướng bộ mô tả tệp Bourne Shell:"
        },
        {
            "bbox": [
                133.64279418286642,
                579.4868702623041,
                261.17070115375054,
                10.225540329193393
            ],
            "text": "$output = ‘$cmd 2>some_file‘;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ output = ‘$ cmd 2> some_file‘;"
        },
        {
            "bbox": [
                133.64279418286642,
                594.0420551492231,
                297.19660762402657,
                10.225528154206302
            ],
            "text": "open (PIPE, \"cmd 2>some_file |\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "mở (ống, \"cmd 2> some_file |\");"
        },
        {
            "bbox": [
                97.61913822327794,
                623.0078834763701,
                606.0588174645039,
                10.916909145887075
            ],
            "text": "You can also use file-descriptor redirection to make STDERR a duplicate of STDOUT:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Bạn cũng có thể sử dụng chuyển hướng của File Descriptor để biến Stderr thành một bản sao của stdout:"
        },
        {
            "bbox": [
                133.64279418286642,
                652.1447703720822,
                198.14250624920206,
                10.225540329193336
            ],
            "text": "$output = ‘$cmd 2>&1‘;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ ra ra = ‘$ cmd 2> & 1;"
        },
        {
            "bbox": [
                133.64279418286642,
                666.6999552590012,
                234.16841271947803,
                10.225528154206302
            ],
            "text": "open (PIPE, \"cmd 2>&1 |\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Mở (ống, \"CMD 2> & 1 |\");"
        },
        {
            "bbox": [
                97.61913822327794,
                695.665783586148,
                93.3928788914722,
                10.916909145887132
            ],
            "text": "Note that you",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Lưu ý rằng bạn"
        },
        {
            "bbox": [
                195.29680349955316,
                695.665783586148,
                46.69644375046195,
                10.916909145887132
            ],
            "text": "cannot",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Italic",
                "size": 9.963000297546387
            },
            "text_vi": "không thể"
        },
        {
            "bbox": [
                246.5884560271202,
                695.665783586148,
                674.0277270543412,
                10.916909145887132
            ],
            "text": "simply open STDERR to be a dup of STDOUT in your Perl program and avoid calling the shell to",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "chỉ cần mở stderr để trở thành một bản sao của stdout trong chương trình perl của bạn và tránh gọi shell đến"
        },
        {
            "bbox": [
                97.61912444815515,
                710.2209684730669,
                259.72967006061054,
                10.91694567084835
            ],
            "text": "do the redirection. This doesn’t work:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Làm chuyển hướng. Điều này không hoạt động:"
        },
        {
            "bbox": [
                133.64279418286642,
                739.3578553687792,
                225.1619447113608,
                10.225528154206245
            ],
            "text": "open(STDERR, \">&STDOUT\");",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "mở (stderr, \"> & stdout\");"
        },
        {
            "bbox": [
                133.64279418286642,
                753.9130524306851,
                216.15545948434007,
                10.225528154206302
            ],
            "text": "$alloutput = ‘cmd args‘;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "$ alloutput = ‘cmd args‘;"
        },
        {
            "bbox": [
                367.81120690234445,
                753.9130524306851,
                8.016639340406812,
                10.225528154206302
            ],
            "text": "#",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "#"
        },
        {
            "bbox": [
                391.09762152346144,
                753.9130524306851,
                174.8388348429972,
                10.225528154206302
            ],
            "text": "stderr still escapes",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
            "text_vi": "Stderr vẫn trốn thoát"
        },
        {
            "bbox": [
                97.61913822327794,
                782.878880757832,
                821.4078950352442,
                10.916945670848378
            ],
            "text": "This fails because the open() makes STDERR go to where STDOUT was going at the time of the open(). The backticks",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
