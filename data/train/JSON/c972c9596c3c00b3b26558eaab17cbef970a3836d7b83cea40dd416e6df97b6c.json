            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
            },
            "text_vi": "Về việc đình chỉ và phần còn lại của thủ tục;"
        },
        {
            "bbox": [
                90.43210989266777,
                182.40953219271262,
                11.29370282495644,
                19.248678133309113
