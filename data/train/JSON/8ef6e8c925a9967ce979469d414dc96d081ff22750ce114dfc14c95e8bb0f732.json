        {
            "bbox": [
                517.2664719432489,
                467.82944976575243,
                152.2220319684153,
                10.570929697018869
            ],
            "text": "well-balanced portfolio. ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/OCPGOB+Tahoma",
                "size": 1
            },
            "text_vi": "Danh mục đầu tư cân bằng tốt. "
        },
        {
            "bbox": [
                73.19347237793664,
                498.8212013200688,
                93.91415962358394,
                10.570929697018869
            ],
            "text": "Brand Image",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/OCPGPD+Tahoma-Bold",
                "size": 1
            },
            "text_vi": "Hình ảnh thương hiệu"
        },
        {
            "bbox": [
                73.19347237793664,
                514.3186203420041,
                410.80551218031286,
                10.570929697018755
            ],
            "text": "The success of a global company is largely dependent on its",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/OCPGOB+Tahoma",
                "size": 1
            },
            "text_vi": "Thành công của một công ty toàn cầu chủ yếu phụ thuộc vào"
        },
        {
            "bbox": [
                73.19347237793664,
                529.8160264497571,
                419.1684456094967,
                10.570929697018755
            ],
            "text": "brand strength and its ability to respond to the desires and ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/OCPGOB+Tahoma",
                "size": 1
            },
            "text_vi": "sức mạnh thương hiệu và khả năng đối phó với mong muốn và "
