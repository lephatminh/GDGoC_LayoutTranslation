        {
            "bbox": [
                140.68627450980392,
                396.48995643939395,
                231.12745098039215,
                10.172335542929318
            ],
            "text": "CREATE TABLE ... SELECT",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
