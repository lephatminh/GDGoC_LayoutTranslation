    "cells": [
        {
            "bbox": [
                394.10580065359477,
                48.370422979798036,
                206.64133986928107,
                11.971275252525288
            ],
            "text": "CREATE TABLE Statement",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Tạo câu lệnh bảng"
        },
        {
            "bbox": [
                156.76470253267973,
                95.32501294191934,
                482.35296127450977,
                8.13782702020194
            ],
            "text": "SELECT artist.name, COUNT(work.artist_id) AS number_of_works",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "Chọn Artist.Name, Count (work.artist_id) là number_of_works"
        },
        {
            "bbox": [
                156.76470253267973,
                107.74921654040418,
                450.1960650326797,
                8.137917613636205
            ],
            "text": "FROM artist LEFT JOIN work ON artist.id = work.artist_id",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "Từ nghệ sĩ rời khỏi công việc trên artist.id = work.artist_id"
        },
        {
            "bbox": [
                156.76470253267973,
                120.17351073232317,
                152.74510138888894,
                8.137827020202167
            ],
            "text": "GROUP BY artist.id;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "Nhóm bởi Artist.id;"
        },
        {
            "bbox": [
                140.68627450980392,
                146.62544760101014,
                571.5882352941176,
                11.971275252525288
            ],
            "text": "You can also explicitly specify the data type for a column in the created table:",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Bạn cũng có thể chỉ định rõ ràng kiểu dữ liệu cho một cột trong bảng đã tạo:"
        },
        {
            "bbox": [
                140.68627450980392,
                174.29141414141418,
                506.4705714869281,
                8.137904671717138
            ],
            "text": "CREATE TABLE foo (a TINYINT NOT NULL) SELECT b+1 AS a FROM bar;",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "Tạo bảng foo (một tinyint không null) Chọn B+1 làm từ thanh;"
        },
        {
            "bbox": [
                140.68627450980392,
                200.74342866161624,
                29.778594771241842,
                11.971275252525288
            ],
            "text": "For ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Vì "
        },
        {
            "bbox": [
                170.46486928104576,
                201.89520770202023,
                231.12745098039215,
                10.172400252525335
            ],
            "text": "CREATE TABLE ... SELECT",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Tạo bảng ... Chọn"
        },
        {
            "bbox": [
                401.5923202614379,
                200.74342866161624,
                22.34232026143792,
                11.971275252525288
            ],
            "text": ", if ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": ", nếu như "
        },
        {
            "bbox": [
                423.93464052287584,
                201.89520770202023,
                130.63725490196077,
                10.172400252525335
            ],
            "text": "IF NOT EXISTS",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Nếu không tồn tại"
        },
        {
            "bbox": [
                554.5718954248366,
                200.74342866161624,
                338.86970629084976,
                11.971275252525288
            ],
            "text": " is given and the target table exists, nothing is",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " được đưa ra và bảng đích tồn tại, không có gì"
        },
        {
            "bbox": [
                140.68627450980392,
                216.27373169191935,
                493.4403594771242,
                11.971275252525174
            ],
            "text": "inserted into the destination table, and the statement is not logged.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "được chèn vào bảng đích và câu lệnh không được ghi lại."
        },
        {
            "bbox": [
                140.68627450980392,
                244.9012569444444,
                720.531387254902,
                11.971275252525288
            ],
            "text": "To ensure that the binary log can be used to re-create the original tables, MySQL does not permit",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Để đảm bảo rằng nhật ký nhị phân có thể được sử dụng để tạo lại các bảng gốc, MySQL không cho phép"
        },
        {
            "bbox": [
                140.68627450980392,
                260.4315599747474,
                188.9550821078431,
                11.971275252525288
            ],
            "text": "concurrent inserts during ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "chèn đồng thời trong "
        },
        {
            "bbox": [
                329.64135661764703,
                261.58332607323246,
                231.12745098039227,
                10.172413194444289
            ],
            "text": "CREATE TABLE ... SELECT",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Tạo bảng ... Chọn"
        },
        {
            "bbox": [
                560.7688075980393,
                260.4315599747474,
                311.83782679738545,
                11.971275252525288
            ],
            "text": ". However, prior to MySQL 8.0.21, when a",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": ". Tuy nhiên, trước MySQL 8.0,21, khi một"
        },
        {
            "bbox": [
                140.68627450980392,
                277.11362910353546,
                231.12745098039215,
                10.172413194444289
            ],
            "text": "CREATE TABLE ... SELECT",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Tạo bảng ... Chọn"
        },
        {
            "bbox": [
                371.8137254901961,
                275.96186300505053,
                536.1654411764705,
                11.971275252525174
            ],
            "text": " operation is applied from the binary log when row-based replication is in",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Hoạt động được áp dụng từ nhật ký nhị phân khi sao chép dựa trên hàng"
        },
        {
            "bbox": [
                140.68627450980392,
                291.49216603535353,
                784.7112430555555,
                11.971275252525174
            ],
            "text": "use, concurrent inserts are permitted on the replicated table while copying data. That limitation is removed",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Sử dụng, chèn đồng thời được cho phép trên bảng sao chép trong khi sao chép dữ liệu. Hạn chế đó đã bị xóa"
        },
        {
            "bbox": [
                140.68627450980392,
                307.02246906565654,
                774.5114379084969,
                11.971275252525288
            ],
            "text": "in MySQL 8.0.21 on storage engines that support atomic DDL. For more information, see Section 13.1.1,",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Trong MySQL 8.0.21 trên các công cụ lưu trữ hỗ trợ DDL nguyên tử. Để biết thêm thông tin, xem Phần 13.1.1,"
        },
        {
            "bbox": [
                140.68627450980392,
                322.55277209595954,
                348.70099714052293,
                11.971275252525288
            ],
            "text": "“Atomic Data Definition Statement Support”",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "“Tuyên bố định nghĩa dữ liệu nguyên tử hỗ trợ”"
        },
        {
            "bbox": [
                461.8194276960784,
                322.55277209595954,
                4.656095996732063,
                11.971275252525288
            ],
            "text": ".",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "."
        },
        {
            "bbox": [
                140.68627450980392,
                351.18028440656565,
                121.04042442810459,
                11.971275252525288
            ],
            "text": "You cannot use ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Bạn không thể sử dụng "
        },
        {
            "bbox": [
                261.7266989379085,
                352.33214109848495,
                100.49019607843138,
                10.172322601010023
            ],
