{
    "metadata": {
        "page_hash": "8f58408a55efef4d34725f5d0ee287edf38e22aa0a74f16a3d3d4b17a2c48d50",
        "original_filename": "user-handbook.pdf",
        "page_no": 109,
        "num_pages": 206,
        "original_width": 595.28003,
