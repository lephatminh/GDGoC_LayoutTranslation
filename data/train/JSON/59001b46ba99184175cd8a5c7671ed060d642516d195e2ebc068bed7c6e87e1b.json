        },
        {
            "bbox": [
                634.446745506536,
                473.77010321969703,
                165.25031576797392,
                11.44941824494947
            ],
            "text": "Recent work on larger",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WNZBVP+CMR10",
                "size": 9.962599754333496
            },
            "text_vi": "Công việc gần đây trên lớn hơn"
