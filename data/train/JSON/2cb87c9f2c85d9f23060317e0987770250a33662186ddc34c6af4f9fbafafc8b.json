            "bbox": [
                734.1531790674603,
                382.26230687830696,
                144.41700892857136,
                21.94804686318969
            ],
            "text": " even to the con‐",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/CPDGGC+MinionPro-Regular",
                "size": 10.5
            },
            "text_vi": " Ngay cả với con lừa"
