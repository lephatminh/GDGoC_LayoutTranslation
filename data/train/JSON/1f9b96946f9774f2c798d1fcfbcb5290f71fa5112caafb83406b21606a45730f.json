        {
            "bbox": [
                205.95766008403362,
                168.54722624703095,
                448.69903865546235,
                11.460936460807602
            ],
            "text": "This appendix explains how to preseed answers to questions in",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/UVLVGJ+NimbusRomNo9L-Regu",
                "size": 9.962639808654785
            },
            "text_vi": "Phụ lục này giải thích cách trả lời câu trả lời cho các câu hỏi trong"
        },
        {
            "bbox": [
                660.9435147058824,
                170.07291805225657,
                148.33992941176473,
                9.321522862232769
            ],
            "text": "debian-installer",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/TCGTYJ+NimbusMonL-Regu",
                "size": 8.96638011932373
            },
