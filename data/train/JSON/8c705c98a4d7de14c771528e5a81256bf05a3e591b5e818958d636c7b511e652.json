        },
        {
            "bbox": [
                561.8562649930453,
                178.47100507820494,
                344.54497283612966,
                10.916994370796715
            ],
            "text": "switch. The editing happens immediately after the",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "công tắc. Việc chỉnh sửa xảy ra ngay sau khi"
        },
        {
            "bbox": [
                97.61911756059375,
                193.02622649008515,
                734.516868939786,
                10.916994370796601
            ],
            "text": "platform specific hints files have been processed but before the actual configuration process starts. Run",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Times-Roman",
                "size": 9.963000297546387
            },
            "text_vi": "Các tệp gợi ý cụ thể của nền tảng đã được xử lý nhưng trước khi quá trình cấu hình thực tế bắt đầu. Chạy"
        },
        {
            "bbox": [
                807.5958462965078,
                193.11115920000054,
                108.07766775411744,
                10.225540329193336
            ],
            "text": "Configure -h",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/ATKAZA+t1xtt",
                "size": 9.963000297546387
            },
