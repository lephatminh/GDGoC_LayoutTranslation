{
    "metadata": {
        "page_hash": "84bdc3e7a5f50135db3cb085e286cca1843a7355a8ae624e5eac6738f1d98491",
        "original_filename": "cloud-for-dummies.pdf",
        "page_no": 15,
        "num_pages": 71,
