            "text": ":",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": ":"
        },
        {
            "bbox": [
                140.68627450980392,
                419.6080808080808,
                56.274509803921575,
                8.137904671717251
            ],
            "text": "mysql> ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 8
            },
            "text_vi": "mysql> "
        },
        {
            "bbox": [
                196.9607843137255,
                419.6080808080808,
                321.5686441993464,
                8.137904671717251
            ],
