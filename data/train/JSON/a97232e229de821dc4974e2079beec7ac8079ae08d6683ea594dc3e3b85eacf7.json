        },
        {
            "bbox": [
                86.9883235231339,
                935.3944678964677,
                13.81209902777374,
                19.248674480808617
            ],
            "text": "(1)",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
            },
            "text_vi": "(1)"
        },
        {
            "bbox": [
                104.34575240533358,
                935.3944678964677,
                443.2662496907731,
                19.248674480808617
            ],
            "text": "Ordnungswidrig handelt, wer vorsätzlich oder fahrlässig",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
