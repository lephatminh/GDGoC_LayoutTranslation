        {
            "bbox": [
                86.9883235231339,
                439.729392229365,
                15.35103417408645,
                19.248678133309227
            ],
            "text": "(5)",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
            },
            "text_vi": "(5)"
        },
        {
            "bbox": [
                106.1471695049458,
                439.729392229365,
                837.2075260272134,
                19.248678133309227
            ],
