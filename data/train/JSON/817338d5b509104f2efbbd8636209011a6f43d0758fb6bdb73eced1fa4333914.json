{
    "metadata": {
        "page_hash": "817338d5b509104f2efbbd8636209011a6f43d0758fb6bdb73eced1fa4333914",
        "original_filename": "1611.06217.pdf",
        "page_no": 25,
        "num_pages": 47,
        "original_width": 595,
