{
    "metadata": {
        "page_hash": "98957fde675dd09aa2f3da141c0060ffe2e1c29719731df3f67d159f65b6f181",
        "original_filename": "1001.4522.pdf",
        "page_no": 8,
        "num_pages": 13,
