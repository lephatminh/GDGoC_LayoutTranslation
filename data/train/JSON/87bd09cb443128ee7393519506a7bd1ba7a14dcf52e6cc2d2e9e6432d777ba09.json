        },
        {
            "bbox": [
                225.18939393939394,
                238.9847961601307,
                376.9282973484848,
                42.57436315359473
            ],
            "text": "Understanding the",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/HADUZR+IScriptSTD",
                "size": 1
            },
            "text_vi": "Hiểu được"
        },
        {
            "bbox": [
                225.18939393939394,
                272.48152818627455,
                331.4166407828282,
                42.57436315359473
            ],
            "text": "benefits of PaaS",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/HADUZR+IScriptSTD",
                "size": 1
            },
