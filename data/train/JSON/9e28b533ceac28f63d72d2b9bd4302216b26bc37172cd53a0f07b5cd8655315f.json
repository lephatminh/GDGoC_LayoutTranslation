            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/FUMJMQ+MyriadPro-Regular",
                "size": 1
            },
            "text_vi": "phân đoạn. Chúng tôi tin rằng sẽ có động lực tiếp tục "
