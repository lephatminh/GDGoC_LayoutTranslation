{
    "metadata": {
        "page_hash": "aeea2024864373ac68374508d36f8bdeff8126de328a822bf35bb2dfb002fca4",
        "original_filename": "TSX-V_CZO_2012.pdf",
        "page_no": 35,
        "num_pages": 58,
        "original_width": 612.23999,
        "original_height": 792.23999,
