            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": " Kết hợp các đối số của nó để tạo ra một chuỗi duy nhất."
        },
        {
            "bbox": [
                140.68627450980392,
                873.4566360479798,
                282.99714052287584,
                11.97127525252526
            ],
            "text": "What collation should the result have?",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Helvetica",
                "size": 10
            },
            "text_vi": "Kết quả nên đối chiếu nào?"
