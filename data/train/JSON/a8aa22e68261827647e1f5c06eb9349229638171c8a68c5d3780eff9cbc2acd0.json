            "bbox": [
                386.468545751634,
                859.0781767676767,
                130.63725490196077,
                10.17233554292929
            ],
            "text": "CONCAT(x,’Y’)",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Courier",
                "size": 10
            },
            "text_vi": "Concat (x, xông y,)"
        },
        {
            "bbox": [
                517.1058006535948,
                857.9263330176768,
                377.9269125816994,
                11.971275252525231
