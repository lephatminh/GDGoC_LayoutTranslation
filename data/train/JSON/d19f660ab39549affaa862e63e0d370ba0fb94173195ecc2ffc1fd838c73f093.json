        },
        {
            "bbox": [
                503.27511723856213,
                679.6738633207071,
                7.092129084967269,
                9.73721527777775
            ],
            "text": "∗",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WYXCVA+CMSY8",
                "size": 7.970099925994873
