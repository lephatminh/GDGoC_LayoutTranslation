{
    "metadata": {
        "page_hash": "313dda9d8437c600451468c437888cc7e534a9a25f7dd80950b4edc33d44eca8",
        "original_filename": "NASDAQ_ATRI_2003.pdf",
        "page_no": 7,
        "num_pages": 32,
        "original_width": 612,
