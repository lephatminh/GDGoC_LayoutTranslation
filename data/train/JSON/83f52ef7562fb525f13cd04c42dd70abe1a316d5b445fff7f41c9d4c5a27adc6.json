            "text": "may still be useful.",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/NFSKTL+CMR12",
                "size": 11.9552001953125
            },
            "text_vi": "vẫn có thể hữu ích."
