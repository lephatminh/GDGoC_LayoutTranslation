        {
            "bbox": [
                277.305119743392,
                717.9633106110856,
                120.32087108650359,
                10.91647084635207
            ],
            "text": "python3.1",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/USPIPX+FreeMono",
                "size": 9.962599754333496
            },
            "text_vi": "Python3.1"
