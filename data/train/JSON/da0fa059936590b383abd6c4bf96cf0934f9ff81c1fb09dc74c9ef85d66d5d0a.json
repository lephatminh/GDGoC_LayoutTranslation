            "bbox": [
                151.42201570242494,
                716.5441332413482,
                8.982639943422924,
                12.129428284818346
            ],
            "text": "•",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/UZCJIF+FreeSerif",
                "size": 9.962599754333496
            },
            "text_vi": "•"
        },
        {
            "bbox": [
                168.6024047371453,
                716.5441332413482,
                103.03869835848516,
                12.129428284818346
            ],
            "text": "The commands",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/UZCJIF+FreeSerif",
                "size": 9.962599754333496
            },
            "text_vi": "Các lệnh"
