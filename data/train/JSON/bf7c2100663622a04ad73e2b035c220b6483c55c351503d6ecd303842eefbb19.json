            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
            },
            "text_vi": "Việc thực hiện và thực hiện các thỏa thuận và các tệp pháp lý theo nghĩa của đoạn 1"
        },
