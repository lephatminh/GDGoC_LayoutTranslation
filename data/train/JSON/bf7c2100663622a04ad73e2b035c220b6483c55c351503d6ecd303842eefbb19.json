            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAA+DejaVuSansCondensed-Bold",
                "size": 10
            },
            "text_vi": "§ 99 vấn đề thời thơ ấu"
        },
        {
            "bbox": [
                86.9883235231339,
                573.9941331100449,
                13.133351118950031,
                19.248665958307186
            ],
            "text": "(1)",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/EAAAAB+DejaVuSansCondensed",
                "size": 10
            },
            "text_vi": "(1)"
        },
