        {
            "bbox": [
                803.7051127450981,
                576.4793222853535,
                73.02932393790843,
                13.02967834595961
            ],
            "text": "image area",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Palatino-Italic",
                "size": 1
            },
            "text_vi": "khu vực hình ảnh"
