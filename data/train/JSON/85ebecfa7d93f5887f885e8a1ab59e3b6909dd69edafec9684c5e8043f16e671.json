        {
            "bbox": [
                876.731070261438,
                576.4793222853535,
                30.662255310457454,
                13.09421969696973
            ],
            "text": ".In",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/Palatino-Roman",
                "size": 1
