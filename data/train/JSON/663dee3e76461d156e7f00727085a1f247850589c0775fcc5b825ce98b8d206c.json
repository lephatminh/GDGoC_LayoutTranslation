        },
        {
            "bbox": [
                18.494119789915967,
                465.61563568883616,
                0.8410443697478982,
                61.30279097387165
            ],
            "text": "WE",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/MENEGB+NissanAG-Regular",
                "size": 1
            },
            "text_vi": "CHÚNG TÔI"
        },
        {
            "bbox": [
                20.45668298319328,
                465.61563568883616,
                1.4017779411764728,
                61.30279097387165
            ],
            "text": "ARE",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/MENEGB+NissanAG-Regular",
                "size": 1
            },
            "text_vi": "LÀ"
        },
        {
            "bbox": [
                292.992718487395,
                253.12663509501192,
                313.48531092436974,
                18.11401425178144
            ],
            "text": "Exceeding expectations",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/MENEGC+NissanAG-Bold",
                "size": 1
            },
            "text_vi": "Vượt quá mong đợi"
        },
        {
            "bbox": [
                292.992718487395,
                279.90810777909735,
                324.4113113445378,
                18.114014251781555
            ],
            "text": "—the Nissan automobile",
            "font": {
