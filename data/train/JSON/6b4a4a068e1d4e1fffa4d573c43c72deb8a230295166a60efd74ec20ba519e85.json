        {
            "bbox": [
                553.913037037037,
                815.0736434108527,
                114.2839970538721,
                15.917958656330768
            ],
            "text": "or feeder route. ",
            "font": {
                "color": [
                    0,
                    0,
                    0,
                    255
                ],
                "name": "/WUSXGL+MyriadPro-Regular",
                "size": 1
            },
            "text_vi": "hoặc tuyến đường trung chuyển. "
        },
        {
            "bbox": [
                937.5805454545456,
                963.7648552325581,
                28.95542171717159,
                15.91795998062014
            ],
            "text": "G-1 ",
            "font": {
                "color": [
                    0,
