{
    "metadata": {
        "page_hash": "dc6ea998dbcfb9b076a0eaf8c92ff0a4ef36f890d640aac4ff05e9d9bd878c95",
        "original_filename": "1001.3882.pdf",
        "page_no": 1,
        "num_pages": 9,
        "original_width": 612,
        "original_height": 792,
        "coco_width": 1025,
